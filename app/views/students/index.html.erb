<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>Matricula</th>
      <th>Nome</th>
      <th>Sexo</th>
      <th>Estado civil</th>
      <th>Filiacao</th>
      <th>Endereco</th>
      <th>Telefone</th>
      <th>Email</th>
      <th>Esporte preferido</th>
      <th>Escolaridade</th>
      <th>Naturalidade</th>
      <th>Cidade</th>
      <th>Uf</th>
      <th>Pais</th>
      <th>Cargo</th>
      <th>Cpf</th>
      <th>Identidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.matricula %></td>
        <td><%= student.nome %></td>
        <td><%= student.sexo %></td>
        <td><%= student.estado_civil %></td>
        <td><%= student.filiacao %></td>
        <td><%= student.endereco %></td>
        <td><%= student.telefone %></td>
        <td><%= student.email %></td>
        <td><%= student.esporte_preferido %></td>
        <td><%= student.escolaridade %></td>
        <td><%= student.naturalidade %></td>
        <td><%= student.cidade %></td>
        <td><%= student.uf %></td>
        <td><%= student.pais %></td>
        <td><%= student.cargo %></td>
        <td><%= student.cpf %></td>
        <td><%= student.identidade %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>
